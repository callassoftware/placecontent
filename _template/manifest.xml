<?xml version="1.0" encoding="UTF-8" ?>
<manifest xmlns:x="http://www.callassoftware.com/cchip/template/manifest/">

	<!--- The XML nodes inside of the "resources" node, will cause pdfToolbox to generate
          additional information inside of the calsDocInfo JSON file. This information can
          be used in your HTML template. -->
	<x:resources>

		<!--- request ink information -->
		<x:inkinfo/>

		<!--- request ink coverage information -->
		<x:inkcov aperture="1mm" bbox="trimbox"/>
		 
		<!--- request information for items found by "Apply to" check -->
		<x:hitinfo/>
		 
	</x:resources>
</manifest>